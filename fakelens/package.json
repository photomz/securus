{
  "name": "songbot-web",
  "description": "Frontend web client for SongBot",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop -H 0.0.0.0",
    "start": "yarn develop",
    "serve": "gatsby serve -H 0.0.0.0",
    "compile": "tsc",
    "test": "jest --watchAll --env=jest-environment-jsdom-sixteen",
    "test:coverage": "jest --watchAll --env=jest-environment-jsdom-sixteen --coverage",
    "test:ci": "jest --ci --env=jest-environment-jsdom-sixteen",
    "lint": "eslint --fix src",
    "format": "prettier --write src",
    "storybook": "NODE_ENV=test start-storybook -p 6006 -s ./static",
    "build-storybook": "NODE_ENV=test build-storybook -s ./static",
    "clean": "gatsby clean"
  },
  "devDependencies": {
    "@storybook/addon-links": "^6.3.4",
